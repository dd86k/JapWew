<Page x:Class="JapWew.SettingsPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:JapWew"
      mc:Ignorable="d" 
      d:DesignHeight="400" d:DesignWidth="600"
      Title="SettingsPage">

    <Grid Background="White">
        <VirtualizingStackPanel Height="50" Margin="0" VerticalAlignment="Top" d:LayoutOverrides="HorizontalAlignment" Orientation="Horizontal">
            <Button x:Name="BackButton" Width="40" Height="40" Style="{DynamicResource MetroCircleButtonStyle}" VerticalAlignment="Center" Click="BackButton_Click" Margin="5">
                <Path Data="M15.41 7.41L14 6l-6 6 6 6 1.41-1.41L10.83 12z" Fill="Black"
                      Margin="-8,-6,0,0"/>
            </Button>
            <Label x:Name="SettingsLabel" Content="Settings" FontSize="29.333" Height="50" HorizontalAlignment="Left" Width="165"/>
        </VirtualizingStackPanel>


    </Grid>
</Page>
